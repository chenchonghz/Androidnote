<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dyk.servicetest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".broadcastpackage.AlarmReceiver"></receiver>
        <service android:name=".servicepackage.MyService"></service>
        <service android:name=".servicepackage.LongRunningService"></service>
        <service android:name="dyk.servicetest.servicepackage.IntentServiewTest"></service>
    </application>

</manifest>